<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.ereporting.EReportingAssessmentType"
           table="assessmenttype">
        <id name="assessmentType"
            type="string">
            <column name="assessmentType" />
            <generator class="assigned" />
        </id>
        <property name="uri"
                  type="string">
            <column name="uri" />
        </property>
    </class>
</hibernate-mapping>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <!-- Encoder implementations from svalbard -->
    <bean class="org.n52.svalbard.encode.OmEncoderv100"/>
    <bean class="org.n52.svalbard.encode.SamplingEncoderv100"/>
    <bean class="org.n52.svalbard.encode.SosV1DescribeSensorResponseEncoder"/>
    <bean class="org.n52.svalbard.encode.SosV1GetCapabilitiesResponseEncoder"/>
    <bean class="org.n52.svalbard.encode.SosV1GetFeatureOfInterestResponseEncoder"/>
    <bean class="org.n52.svalbard.encode.SosV1GetObservationByIdResponseEncoder"/>
    <bean class="org.n52.svalbard.encode.SosV1GetObservationResponseEncoder"/>

    <!-- Decoder implementations from svalbard -->
    <bean class="org.n52.svalbard.decode.SosDecoderv100"/>
    <bean class="org.n52.svalbard.decode.OgcDecoderv100"/>
</beans>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:ctx="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.2.xsd">

    <!-- ServiceEventListener implementations from extensions/do/core -->
    <bean id="deleteObservationContentModificationListener"
          class="org.n52.sos.ext.deleteobservation.DeleteObservationContentModificationListener" />

    <!-- ServiceEventListener implementations from core/cache -->
    <bean id="contentModificationListener"
          class="org.n52.sos.cache.ctrl.DefaultContentModificationListener" />


</beans>

<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean class="org.n52.faroe.settings.BooleanSettingDefinition">
        <property name="key" value="service.observation.onlineResource" />
        <property name="title" value="Should the SOS encode CI_OnlineResource in observations?" />
        <property name="description" value="Activate/Deactivate whether the service should encode &lt;tt&gt;CI_OnlineResource&lt;/tt&gt; in observations." />
        <property name="order" value="0.0" />
        <property name="group" ref="miscellaneousSettingDefinitionGroup" />
        <property name="defaultValue" value="false" />
    </bean>

</beans>

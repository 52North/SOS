<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.Codespace">
		<subselect>(SELECT 1 AS codespaceId, 'http://www.opengis.net/def/nil/OGC/0/unknown' AS codespace)</subselect>
        <id name="codespaceId" type="long" />
        <property name="codespace" type="string" insert="false" update="false" />
    </class>
</hibernate-mapping>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <bean class="org.n52.sos.encode.json.base.TimeJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.BigIntegerJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.BigDecimalJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.StringJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.IntegerJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.BooleanJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.FloatJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.DoubleJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.LongJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.URIJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.URLJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.ByteJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.base.ShortJSONEncoder"/>

</beans>

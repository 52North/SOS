<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <!-- OperationHandler implementations from core/api -->
    <bean class="org.n52.sos.ds.BatchOperationHandler"/>
    <!-- OperationHandler implementations from core/gc-dao -->
    <bean class="org.n52.sos.ds.GetCapabilitiesDAO"/>
    <!-- OperationHandler implementations from hibernate/dao -->
    <bean class="org.n52.sos.ds.hibernate.DescribeSensorDAO"/>
    <bean class="org.n52.sos.ds.hibernate.GetObservationDAO"/>
    <bean class="org.n52.sos.ds.hibernate.GetFeatureOfInterestDAO"/>
    <bean class="org.n52.sos.ds.hibernate.GetObservationByIdDAO"/>
    <bean class="org.n52.sos.ds.hibernate.InsertResultTemplateDAO"/>
    <bean class="org.n52.sos.ds.hibernate.InsertResultDAO"/>
    <bean class="org.n52.sos.ds.hibernate.GetResultTemplateDAO"/>
    <bean class="org.n52.sos.ds.hibernate.GetResultDAO"/>
    <bean class="org.n52.sos.ds.hibernate.DeleteSensorDAO"/>
    <bean class="org.n52.sos.ds.hibernate.InsertSensorDAO"/>
    <bean class="org.n52.sos.ds.hibernate.UpdateSensorDescriptionDAO"/>
    <bean class="org.n52.sos.ds.hibernate.InsertObservationDAO"/>
    <!-- OperationHandler implementations from extensions/do/hibernate -->
    <bean class="org.n52.sos.ext.deleteobservation.DeleteObservationDAO"/>
    <!-- OperationHandler implementations from extensions/gda/hibernate -->
    <bean class="org.n52.sos.ds.hibernate.GetDataAvailabilityDAO"/>

</beans>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
    <!--
        org.n52.iceland.cache.ContentCacheController
    -->

    <!-- ContentCacheController implementations from core/cache -->
    <bean class="org.n52.sos.cache.ctrl.ContentCacheControllerImpl"/>

    <!--
        org.n52.iceland.ds.CacheFeederHandler
    -->

    <!-- CacheFeederHandler implementations from hibernate/dao -->
    <bean class="org.n52.sos.ds.hibernate.SosCacheFeederDAO"/>

</beans>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <!-- RequestOperator implementations from operations/resultHandling-v20 -->
    <bean class="org.n52.sos.request.operator.SosGetResultTemplateOperatorV20"/>
    <bean class="org.n52.sos.request.operator.SosGetResultOperatorV20"/>
    <beans profile="transactional,ereporting">
        <bean class="org.n52.sos.request.operator.SosInsertResultTemplateOperatorV20"/>
        <bean class="org.n52.sos.request.operator.SosInsertResultOperatorV20"/>
    </beans>
</beans>

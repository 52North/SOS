<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <!-- Encoder implementations from coding/sensorML-v20 -->
    <bean class="org.n52.sos.encode.SensorMLEncoderv20"/>
    <bean class="org.n52.sos.encode.Iso19139GmdEncoder"/>
    <bean class="org.n52.sos.encode.Iso19139GcoEncoder"/>

    <!-- Decoder implementations from coding/sensorML-v20 -->
    <bean class="org.n52.sos.decode.SensorMLDecoderV20"/>
    <bean class="org.n52.sos.decode.Iso19139GmdDecoder"/>
    <bean class="org.n52.sos.decode.Iso19139GcoDecoder"/>

    <!-- Converter implementations from coding/sensorML-v20 -->
    <bean class="org.n52.sos.convert.SensorML20UrlMimeTypeConverter"/>
    <bean class="org.n52.sos.convert.SensorML20SensorML101Converter"/>
</beans>

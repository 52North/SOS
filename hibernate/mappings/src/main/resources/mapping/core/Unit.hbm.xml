<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.Unit">
        <subselect>
            SELECT 1 AS unitId, "cm" AS unit
            UNION
            SELECT 2 AS unitId, "m"  AS unit
        </subselect>
        <id name="unitId" type="long" />
        <property name="unit" type="string"/>
    </class>
</hibernate-mapping>

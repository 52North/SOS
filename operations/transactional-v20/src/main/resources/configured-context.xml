<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <!-- RequestOperator implementations from operations/transactional-v20 -->
    <beans profile="transactional,ereporting">
        <bean class="org.n52.sos.request.operator.SosDeleteSensorOperatorV20"/>
        <bean class="org.n52.sos.request.operator.SosInsertSensorOperatorV20"/>
        <bean class="org.n52.sos.request.operator.SosUpdateSensorDescriptionOperatorV20"/>
        <bean class="org.n52.sos.request.operator.SosInsertObservationOperatorV20"/>
    </beans>
</beans>

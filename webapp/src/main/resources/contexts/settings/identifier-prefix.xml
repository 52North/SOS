<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="identifierPrefixDefinitionGroup" class="org.n52.faroe.SettingDefinitionGroup">
        <property name="title" value="Identifier Prefix" />
        <property name="order" value="10.0" />
    </bean>
    <bean class="org.n52.faroe.settings.StringSettingDefinition">
        <property name="key" value="sos.prefix.offering" />
        <property name="title" value="offering prefix" />
        <property name="description" value="offering prefix" />
        <property name="order" value="1.0" />
        <property name="group" ref="identifierPrefixDefinitionGroup" />
        <property name="optional" value="true" />
    </bean>
    <bean class="org.n52.faroe.settings.StringSettingDefinition">
        <property name="key" value="sos.prefix.obervableProperty" />
        <property name="title" value="obserProp prefix" />
        <property name="description" value="obserProp prefix" />
        <property name="order" value="3.0" />
        <property name="group" ref="identifierPrefixDefinitionGroup" />
        <property name="optional" value="true" />
    </bean>
    <bean class="org.n52.faroe.settings.StringSettingDefinition">
        <property name="key" value="sos.prefix.global" />
        <property name="title" value="global prefix" />
        <property name="description" value="global prefix" />
        <property name="order" value="0.0" />
        <property name="group" ref="identifierPrefixDefinitionGroup" />
        <property name="optional" value="true" />
    </bean>
    <bean class="org.n52.faroe.settings.StringSettingDefinition">
        <property name="key" value="sos.prefix.featureOfInterest" />
        <property name="title" value="feature prefix" />
        <property name="description" value="feature prefix" />
        <property name="order" value="4.0" />
        <property name="group" ref="identifierPrefixDefinitionGroup" />
        <property name="optional" value="true" />
    </bean>
    <bean class="org.n52.faroe.settings.StringSettingDefinition">
        <property name="key" value="sos.prefix.procedure" />
        <property name="title" value="procedure prefix" />
        <property name="description" value="procedure prefix" />
        <property name="order" value="2.0" />
        <property name="group" ref="identifierPrefixDefinitionGroup" />
        <property name="optional" value="true" />
    </bean>
</beans>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.i18n.HibernateI18NObservablePropertyMetadata" table="i18nobservableProperty">
        <id name="id" type="long">
            <column name="id" />
            <generator class="native">
            	<param name="sequence">i18nObsPropId_seq</param>
            </generator>
        </id>
        <many-to-one name="objectId" class="org.n52.sos.ds.hibernate.entities.ObservableProperty" fetch="select" lazy="no-proxy" foreign-key="i18nObsPropObsPropFk">
           	<column index="i18nObsPropIdx" name="objectId" not-null="true" unique-key="i18nobsPropIdentity" />
        </many-to-one>
        <property name="locale" type="locale">
            <column name="locale" />
        </property>
        <property name="name" type="string">
            <column name="name" />
        </property>
        <property name="description" type="string">
            <column name="description" />
        </property>
	</class>
</hibernate-mapping>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">

    <!-- Encoder implementation from svalbard -->
    <bean id="inspireXmlEncoder"
          class="org.n52.svalbard.encode.InspireXmlEncoder"/>

    <bean id="inspireHelper" class="org.n52.sos.inspire.InspireHelper" />

    <bean id="inspireExtendedCapabilitiesProvider"
          class="org.n52.sos.inspire.capabilities.InspireExtendedCapabilitiesProvider"/>

    <bean id="inspireOfferingExtensionProvider"
          class="org.n52.sos.inspire.offering.InspireOfferingExtensionProvider"/>

</beans>

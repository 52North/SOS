<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <!-- Decoder implementations from coding/sensorML-v101 -->
    <bean class="org.n52.sos.decode.SensorMLDecoderV101"/>
    <bean class="org.n52.sos.decode.SweCommonDecoderV101"/>
    <bean class="org.n52.sos.decode.GmlDecoderv311"/>

    <!-- Encoder implementations from coding/sensorML-v101 -->
    <bean class="org.n52.sos.encode.SensorMLEncoderv101"/>
    <bean class="org.n52.sos.encode.GmlEncoderv311"/>
    <bean class="org.n52.sos.encode.SweCommonEncoderv101"/>

        <!-- Converter implementations from coding/sensorML-v101 -->
    <bean class="org.n52.sos.convert.SensorMLUrlMimeTypeConverter"/>

</beans>

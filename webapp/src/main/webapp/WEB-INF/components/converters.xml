<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
    <!--
        org.n52.iceland.convert.Converter
    -->

    <!-- Converter implementations from coding/sensorML-v20 -->
    <bean class="org.n52.sos.convert.SensorML20UrlMimeTypeConverter"/>
    <bean class="org.n52.sos.convert.SensorML20SensorML101Converter"/>
    <!-- Converter implementations from coding/wml-v20 -->
    <bean class="org.n52.sos.convert.WaterMLv20SensorMLv101Converter"/>
    <!-- Converter implementations from coding/sensorML-v101 -->
    <bean class="org.n52.sos.convert.SensorMLUrlMimeTypeConverter"/>

</beans>

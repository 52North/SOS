    <bean class="org.n52.sos.decode.json.impl.GetObservationByIdRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.GetObservationRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.GetResultRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.GetResultTemplateRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.InsertObservationRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.InsertResultRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.InsertResultTemplateRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.InsertSensorRequestDecoder"/>
    <bean class="org.n52.sos.decode.json.impl.InspireIDJSONDecoder"/>

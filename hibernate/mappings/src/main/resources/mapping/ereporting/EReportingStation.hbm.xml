<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.ereporting.EReportingStation" table="Station">
        <id name="stationId" type="long">
            <column name="Id"/>
            <generator class="assigned" />
        </id>
       <property name="identifier" type="string">
            <column name="inspireId" not-null="true" unique-key="stationIdentifierUK"/>
        </property>
        <many-to-one name="codespace" class="org.n52.sos.ds.hibernate.entities.Codespace" fetch="select" lazy="no-proxy" foreign-key="stationCodespaceIdFk">
            <formula>null</formula>
        </many-to-one>
        <property name="name" type="string">
            <column name="name" />
        </property>
        <many-to-one name="codespaceName" class="org.n52.sos.ds.hibernate.entities.Codespace" fetch="select" foreign-key="stationCodespaceNameFk">
            <formula>null</formula>
        </many-to-one>
        <property name="description" type="string">
            <formula>null</formula>
        </property>
    </class>
</hibernate-mapping>

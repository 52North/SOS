<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <!--
        org.n52.iceland.binding.Binding
    -->

    <!-- Binding implementations from webapp -->
    <bean class="org.n52.iceland.binding.kvp.KvpBinding"/>
    <bean class="org.n52.iceland.binding.pox.PoxBinding"/>
    <bean class="org.n52.iceland.binding.json.JSONBinding"/>
    <!-- Binding implementations from bindings/rest/code -->
    <bean class="org.n52.sos.binding.rest.RestBinding"/>
    <!-- Binding implementations from bindings/exi -->
    <bean class="org.n52.sos.binding.EXIBinding"/>
    <!-- Binding implementations from bindings/soap -->
    <bean class="org.n52.sos.binding.SoapBinding"/>

</beans>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <!-- Decoder implementations from coding/sos-v20 -->
    <!-- request decoder -->
    <bean class="org.n52.sos.decode.FesDecoderv20"/>
    <bean class="org.n52.sos.decode.GmlDecoderv321"/>
    <bean class="org.n52.sos.decode.SosDecoderv20"/>
    <bean class="org.n52.sos.decode.SosStringDecoderv20"/>
    <bean class="org.n52.sos.decode.SwesDecoderv20"/>
    <bean class="org.n52.sos.decode.SwesStringDecoderv20" />
    <bean class="org.n52.sos.decode.SweCommonDecoderV20"/>
    <bean class="org.n52.sos.decode.OmDecoderv20"/>
    <bean class="org.n52.sos.decode.SamplingDecoderv20"/>
    <bean class="org.n52.sos.decode.SwesExtensionDecoderv20"/>
    <bean class="org.n52.sos.decode.XlinkDecoderv1999"/>
    <!-- response decoder -->
    <bean class="org.n52.sos.decode.sos.v2.CapabilitiesDocumentDecoder"/>
    <bean class="org.n52.sos.decode.sos.v2.CapabilitiesTypeDecoder"/>
    <bean class="org.n52.sos.decode.sos.v2.GetObservationResponseDocumentDecoder"/>
    

    <!-- Encoder implementations from coding/sos-v20 -->
    <!-- response encoder -->
    <bean class="org.n52.sos.encode.FesEncoderv20"/>
    <bean class="org.n52.sos.encode.GmlEncoderv321"/>
    <bean class="org.n52.sos.encode.OmEncoderv20"/>
    <bean class="org.n52.sos.encode.SamplingEncoderv20"/>
    <bean class="org.n52.sos.encode.SosRequestEncoderv20"/>
    <bean class="org.n52.sos.encode.SweCommonEncoderv20"/>
    <bean class="org.n52.sos.encode.XlinkEncoderv1999"/>
    <bean class="org.n52.sos.encode.swes.DeleteSensorResponseEncoder"/>
    <bean class="org.n52.sos.encode.swes.DescribeSensorResponseEncoder"/>
    <bean class="org.n52.sos.encode.swes.InsertSensorResponseEncoder"/>
    <bean class="org.n52.sos.encode.swes.UpdateSensorResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.GetCapabilitiesResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.GetFeatureOfInterestResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.GetObservationByIdResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.GetObservationResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.GetResultResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.GetResultTemplateResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.InsertObservationResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.InsertResultResponseEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.InsertResultTemplateResponseEncoder"/>
    
    <!-- request encoder -->
    <bean class="org.n52.sos.encode.sos.v2.GetCapabilitiesRequestEncoder"/>
    <bean class="org.n52.sos.encode.sos.v2.GetObservationRequestEncoder"/>
     

</beans>

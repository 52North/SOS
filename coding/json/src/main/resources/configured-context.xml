    <bean class="org.n52.sos.encode.json.impl.BatchResponseEncoder"/>
    <bean class="org.n52.sos.encode.json.impl.CodeTypeJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.impl.ContactJSONEncoder"/>
    <bean class="org.n52.sos.encode.json.impl.DeleteObservationJsonEncoder"/>

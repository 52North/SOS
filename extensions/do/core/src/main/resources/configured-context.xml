<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">

    <bean id="deleteObservationDecoder"
          class="org.n52.sos.ext.deleteobservation.DeleteObservationDecoder"/>

    <bean id="deleteObservationEncoder"
          class="org.n52.sos.ext.deleteobservation.DeleteObservationEncoder"/>

    <bean id="deleteObservationContentModificationListener"
          class="org.n52.sos.ext.deleteobservation.DeleteObservationContentModificationListener" />

    <bean id="deleteObservationRequestOperator"
          class="org.n52.sos.ext.deleteobservation.DeleteObservationRequestOperator"/>

</beans>

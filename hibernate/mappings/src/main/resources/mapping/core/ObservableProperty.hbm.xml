<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.ObservableProperty" table="Pollutant">
        <id name="observablePropertyId" type="long">
            <column name="Id" />
            <generator class="assigned">
            </generator>
        </id>
        <discriminator formula="(SELECT DISTINCT 'F')" type="org.hibernate.type.TrueFalseType"/>
        <property name="identifier" type="string">
            <column name="URI" not-null="true" unique="true" />
        </property>
		<many-to-one name="codespace"
			class="org.n52.sos.ds.hibernate.entities.Codespace" fetch="select"
			foreign-key="featureCodespaceFk">
			<formula>1</formula>
		</many-to-one>
		<property name="name" type="string">
            <column name="notation" />
        </property>
        <many-to-one name="codespaceName"
			class="org.n52.sos.ds.hibernate.entities.Codespace" fetch="select"
			foreign-key="featureCodespaceFk">
			<formula>1</formula>
		</many-to-one>
        <property name="description" type="string">
            <column name="label" />
        </property>
		<property name="disabled" type="org.hibernate.type.TrueFalseType">
            <formula>'F'</formula>
        </property>
    </class>
    
</hibernate-mapping>

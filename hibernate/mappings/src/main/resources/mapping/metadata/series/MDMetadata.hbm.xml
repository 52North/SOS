<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.metadata.MDMetadata"
           table="metadata">
        <id name="metadataId"
            type="long">
            <column name="metadataId">
            	<comment>Table primary key, used for relations</comment>
            </column>
            <generator class="native">
                <param name="sequence">metadataId_seq</param>
            </generator>
        </id>
        <many-to-one name="series"
                     class="org.n52.sos.ds.hibernate.entities.observation.series.Series"
                     fetch="select"
                     lazy="no-proxy"
                     foreign-key="metaDataSeriesFk">
            <column index="metaDataSeriesIdx"
                    name="seriesId"
                    not-null="true">
            </column>
        </many-to-one>
    </class>
</hibernate-mapping>

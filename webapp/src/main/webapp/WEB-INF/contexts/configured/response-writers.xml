<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <!-- ResponseWriterFactory implementations from bindings/soap -->
    <bean class="org.n52.sos.encode.SoapResponseWriterFactory"/>
    <bean class="org.n52.sos.encode.SoapChainResponseWriterFactory"/>
    <!-- ResponseWriterFactory implementations from coding/json -->
    <bean class="org.n52.sos.encode.json.JSONResponseWriterFactory"/>
    <!-- ResponseWriterFactory implementations from coding/exi -->
    <bean class="org.n52.sos.encode.EXIResponseWriterFactory"/>
    <!-- ResponseWriterFactory implementations from core/api -->
    <bean class="org.n52.sos.coding.encode.XmlResponseWriterFactory" />
    <bean class="org.n52.sos.coding.encode.AbstractServiceResponseWriterFactory"/>
    <bean class="org.n52.sos.coding.encode.BinaryAttachmentResponseWriterFactory"/>

</beans>
